{
    "env": {
        "node": true,
        "commonjs": true
    },
    "extends": "eslint:recommended",


    "rules": {
        "indent": [
            "error",
            "tab"

        ],
        "linebreak-style": [
            "error",
            "unix"

        ],
        "quotes": [
            "error",
            "single"

        ]
    },
    "parserOptions": {
        "ecmaVersion": 2016,
        "autoFix": false
    }
}
